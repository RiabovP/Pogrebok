<?xml version="1.0" encoding="utf-8" ?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
  <item
    android:state_focused="false"
        android:state_pressed="false">
    <shape
        android:shape="rectangle">
      <stroke
        android:width="1dp"
        android:color="#ffffff">
      </stroke>
      <corners
         android:radius="15dp">
      </corners>
      <gradient
  android:startColor="#1a89d9"
  android:endColor="#69b8c6">
      </gradient>

      <!--<solid
    android:color="#ffffff">
      </solid>-->
    </shape>
  </item>
  <item
  android:state_focused="false"
      android:state_pressed="true">
    <shape
        android:shape="rectangle">
      <stroke
        android:width="1dp"
        android:color="#805c6769">
      </stroke>
      <corners
         android:radius="10dp">
      </corners>

      <!--<solid
    android:color="#ffffff">
      </solid>-->
    </shape>
  </item>
</selector>
